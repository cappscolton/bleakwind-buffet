<UserControl x:Class="PointOfSale.CashPaymentComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale"
             xmlns:cashPayment="clr-namespace:PointOfSale.CashPayment"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <cashPayment:CurrencyControl Grid.Row="0"/>
        <cashPayment:CurrencyControl Grid.Row="1"/>
        <cashPayment:CurrencyControl Grid.Row="2"/>
        <cashPayment:CurrencyControl Grid.Row="3"/>
        <cashPayment:CurrencyControl Grid.Row="4"/>
        <cashPayment:CurrencyControl Grid.Row="5"/>

    </Grid>
</UserControl>
